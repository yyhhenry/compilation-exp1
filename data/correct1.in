var
    i, j1, sum: integer; flag: bool;

begin
    i := 0; // 0 is valid
    j1 := 50;
    sum := 1;
    flag := 1;
    while (i < j1) and (flag) do
    begin
        i := i + 1;
        sum := sum * i;
        if (sum > 65535 / (i + 1)) then
            flag := 0;
    end;
end

